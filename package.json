{
  "name": "n8n-starter",
  "type": "module",
  "private": true,
  "version": "1.0.1",
  "scripts": {
    "init": "bun bin/init.ts",
    "build": "bun bin/build.ts",
    "generate": "bun bin/generate.ts",
    "generate:build": "bun bin/generate.ts && bun bin/build.ts",
    "version:update": "bun bin/version_update.ts",
    "publish": "git add -A && git commit -m 'update version' && bun run version:update && bun run build && bun bin/publish.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "express": "^5.1.0",
    "@types/bun": "latest",
    "@types/lodash": "^4.17.20",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/ssh2": "^1.15.5",
    "n8n-core": "^1.117.1",
    "n8n-workflow": "^1.116.0",
    "nock": "^14.0.10",
    "ssh2": "^1.17.0",
    "dedent": "^1.7.0",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}